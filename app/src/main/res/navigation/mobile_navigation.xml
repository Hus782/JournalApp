<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.journal22.ui.entries.EntriesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_entries" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.journal22.ui.calendar.CalendarFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.journal22.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/display_entry2"
        android:name="com.example.journal22.display_entry"
        android:label="display_entry" />
</navigation>