<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/main_fragment">
    <fragment
        android:id="@+id/main_fragment"
        android:name="com.example.journal22.main_fragment"
        android:label="main_fragment" >
        <action
            android:id="@+id/action_main_fragment_to_display_entry"
            app:destination="@id/display_entry" />
        <action
            android:id="@+id/action_main_fragment_to_new_entry"
            app:destination="@id/new_entry" />
        <action
            android:id="@+id/action_main_fragment_to_template_manager"
            app:destination="@id/template_manager" />
    </fragment>
    <activity
        android:id="@+id/calendar_view"
        android:name="com.example.journal22.old.calendar_view"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.example.journal22.ui.notifications.NotificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.journal22.ui.entries.EntriesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_entries" >
    </fragment>
    <fragment
        android:id="@+id/display_entry"
        android:name="com.example.journal22.display_entry"
        android:label="display_entry" />
    <fragment
        android:id="@+id/new_entry"
        android:name="com.example.journal22.new_entry"
        android:label="new_entry_fragment"
        tools:layout="@layout/new_entry_fragment" >
        <action
            android:id="@+id/action_new_entry_to_main_fragment"
            app:destination="@id/main_fragment" />
    </fragment>
    <fragment
        android:id="@+id/template_manager"
        android:name="com.example.journal22.ui.templates.template_manager"
        android:label="template_manager_fragment"
        tools:layout="@layout/template_manager_fragment" />

</navigation>